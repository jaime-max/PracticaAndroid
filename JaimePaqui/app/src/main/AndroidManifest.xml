<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jaimepaqui">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JaimePaqui">
        <activity android:name=".ActivityVolleyAlumno">

        </activity>
        <activity android:name="com.example.jaimepaqui.ActivityBDDocente"/>
        <activity android:name="com.example.jaimepaqui.ActivityMemoriaExterna"/>
        <activity android:name="com.example.jaimepaqui.ActivityMemoriaInterna"/>
        <activity android:name="com.example.jaimepaqui.ActivityMemoriaPrograma"/>
        <activity
            android:name="com.example.jaimepaqui.PrincipalActivity"
            android:label="@string/title_activity_principal"
            android:theme="@style/Theme.JaimePaqui.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.jaimepaqui.NombresActivity"
            android:label="@string/title_activity_nombres"
            android:theme="@style/Theme.JaimePaqui.NoActionBar" />
        <activity
            android:name="com.example.jaimepaqui.FragmentosActivity"
            android:label="@string/title_activity_fragmentos"
            android:theme="@style/Theme.JaimePaqui.NoActionBar" />
        <activity android:name="com.example.jaimepaqui.LoginActivity" />
        <activity android:name="com.example.jaimepaqui.MainActivity"/>
    </application>

</manifest>